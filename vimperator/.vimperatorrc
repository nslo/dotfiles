set hintchars=asdflkjghqwertpoiuyzxcvbmn

ignorekeys add -except=J,K /mail\.google\.com/
ignorekeys add /docs\.google\.com/

nmap J <C-PageDown>
nmap K <C-PageUp>
nmap d <C-w>

map <C-q> <Nop>
imap <C-q> <Nop>
cmap <C-q> <Nop>
map <C-k> <Nop>
map <C-j> <Nop>

noremap <C-Tab> i<C-Tab>
cnoremap <C-Tab> i<C-Tab>
inoremap <C-Tab> i<C-Tab

highlight Hint font-family: "DejaVu Sans Mono"; font-size: 12px; font-weight: bold; text-transform: uppercase; color: white; background-color: red; padding: 2px
